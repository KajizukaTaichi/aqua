player = class{
    name; hp; level;

    get-name = method() { name };
    get-hp = method() { hp };
    get-level = method() { level };

    level-up = method(plus) {
        self level = level + 1
    };

    __display__ = method() {
        hp = "HP" + (hp to-string);
        level = "Level" + (level to-string);
        name + (" " + (hp + (" " + level)))
    }
};


you = player{ name = "Mario"; hp = 80; level = 1 };
console writeln you;
you = you level-up;
console writeln you
